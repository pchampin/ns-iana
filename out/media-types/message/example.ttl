PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#message>
  reg:record <example#Resource>.

<example> a rdfs:Class;
  dct:identifier "message/example";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/message/example>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc4735/>;
  <http://www.w3.org/2002/07/owl#sameAs> <example#Resource>.

<example#Resource>
  reg:file [
      reg:file "message/example";
      reg:type "template"];
  reg:name "example";
  reg:xref <https://tools.ietf.org/html/rfc4735>.
