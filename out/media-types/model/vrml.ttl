PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#model>
  reg:record <vrml#Resource>.

<vrml> a rdfs:Class;
  dct:identifier "model/vrml";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/model/vrml>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc2077/>;
  <http://www.w3.org/2002/07/owl#sameAs> <vrml#Resource>.

<vrml#Resource>
  reg:file [
      reg:file "model/vrml";
      reg:type "template"];
  reg:name "vrml";
  reg:xref <https://tools.ietf.org/html/rfc2077>.
