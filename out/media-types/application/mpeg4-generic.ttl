PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#application>
  reg:record <mpeg4-generic#Resource>.

<mpeg4-generic> a rdfs:Class;
  dct:identifier "application/mpeg4-generic";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/application/mpeg4-generic>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc3640/>;
