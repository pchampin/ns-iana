PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#application>
  reg:record <index#Resource>.

<index> a rdfs:Class;
  dct:identifier "application/index";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/application/index>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc2652/>;
  <http://www.w3.org/2002/07/owl#sameAs> <index#Resource>.

<index#Resource>
  reg:file [
      reg:file "application/index";
      reg:type "template"];
