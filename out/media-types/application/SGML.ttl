PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#application>
  reg:record <SGML#Resource>.

<SGML> a rdfs:Class;
  dct:identifier "application/SGML";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/application/SGML>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc1874/>;
  <http://www.w3.org/2002/07/owl#sameAs> <SGML#Resource>.

<SGML#Resource>
  reg:file [
      reg:file "application/SGML";
      reg:type "template"];
  reg:name "SGML";
  reg:xref <https://tools.ietf.org/html/rfc1874>.
