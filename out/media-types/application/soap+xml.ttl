PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#application>
  reg:record <soap+xml#Resource>.

<soap+xml> a rdfs:Class;
  dct:identifier "application/soap+xml";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/application/soap+xml>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc3902/>;
  <http://www.w3.org/2002/07/owl#sameAs> <soap+xml#Resource>.

<soap+xml#Resource>
  reg:file [
      reg:file "application/soap+xml";
      reg:type "template"];
  reg:name "soap+xml";
  reg:xref <https://tools.ietf.org/html/rfc3902>.
