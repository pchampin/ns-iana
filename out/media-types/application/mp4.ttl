PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#application>
  reg:record <mp4#Resource>.

<mp4> a rdfs:Class;
  dct:identifier "application/mp4";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/application/mp4>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc4337/>,
    <https://datatracker.ietf.org/doc/rfc6381/>;
  <http://www.w3.org/2002/07/owl#sameAs> <mp4#Resource>.

<mp4#Resource>
  reg:file [
      reg:file "application/mp4";
      reg:type "template"];
  reg:name "mp4";
  reg:xref <https://tools.ietf.org/html/rfc4337>,
    <https://tools.ietf.org/html/rfc6381>.
