PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#application>
  reg:record <dvcs#Resource>.

<dvcs> a rdfs:Class;
  dct:identifier "application/dvcs";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/application/dvcs>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc3029/>;
  <http://www.w3.org/2002/07/owl#sameAs> <dvcs#Resource>.

<dvcs#Resource>
  reg:file [
      reg:file "application/dvcs";
      reg:type "template"];
  reg:name "dvcs";
  reg:xref <https://tools.ietf.org/html/rfc3029>.
