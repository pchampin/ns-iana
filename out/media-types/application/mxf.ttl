PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#application>
  reg:record <mxf#Resource>.

<mxf> a rdfs:Class;
  dct:identifier "application/mxf";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/application/mxf>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc4539/>;
  <http://www.w3.org/2002/07/owl#sameAs> <mxf#Resource>.

<mxf#Resource>
  reg:file [
      reg:file "application/mxf";
      reg:type "template"];
  reg:name "mxf";
  reg:xref <https://tools.ietf.org/html/rfc4539>.
