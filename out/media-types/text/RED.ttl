PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#text>
  reg:record <RED#Resource>.

<RED> a rdfs:Class;
  dct:identifier "text/RED";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/text/RED>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc4102/>;
  <http://www.w3.org/2002/07/owl#sameAs> <RED#Resource>.

<RED#Resource>
  reg:file [
      reg:file "text/RED";
      reg:type "template"];
  reg:name "RED";
  reg:xref <https://tools.ietf.org/html/rfc4102>.
