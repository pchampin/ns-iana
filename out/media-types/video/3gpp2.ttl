PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#video>
  reg:record <3gpp2#Resource>.

<3gpp2> a rdfs:Class;
  dct:identifier "video/3gpp2";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/video/3gpp2>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc4393/>,
    <https://datatracker.ietf.org/doc/rfc6381/>;
  <http://www.w3.org/2002/07/owl#sameAs> <3gpp2#Resource>.

<3gpp2#Resource>
  reg:file [
      reg:file "video/3gpp2";
      reg:type "template"];
  reg:name "3gpp2";
  reg:xref <https://tools.ietf.org/html/rfc4393>,
    <https://tools.ietf.org/html/rfc6381>.
