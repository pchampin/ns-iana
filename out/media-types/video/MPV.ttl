PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#video>
  reg:record <MPV#Resource>.

<MPV> a rdfs:Class;
  dct:identifier "video/MPV";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/video/MPV>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc3555/>;
  <http://www.w3.org/2002/07/owl#sameAs> <MPV#Resource>.

<MPV#Resource>
  reg:file [
      reg:file "video/MPV";
      reg:type "template"];
  reg:name "MPV";
  reg:xref <https://tools.ietf.org/html/rfc3555>.
