PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#video>
  reg:record <mj2#Resource>.

<mj2> a rdfs:Class;
  dct:identifier "video/mj2";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/video/mj2>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc3745/>;
  <http://www.w3.org/2002/07/owl#sameAs> <mj2#Resource>.

<mj2#Resource>
  reg:file [
      reg:file "video/mj2";
      reg:type "template"];
  reg:name "mj2";
  reg:xref <https://tools.ietf.org/html/rfc3745>.
