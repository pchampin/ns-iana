<PCMA> a rdfs:Class;
  dct:identifier "audio/PCMA";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/audio/PCMA>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc4856/>;
