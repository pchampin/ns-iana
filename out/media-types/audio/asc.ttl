PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#audio>
  reg:record <asc#Resource>.

<asc> a rdfs:Class;
  dct:identifier "audio/asc";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/audio/asc>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc6295/>;
  <http://www.w3.org/2002/07/owl#sameAs> <asc#Resource>.

<asc#Resource>
  reg:file [
      reg:file "audio/asc";
      reg:type "template"];
  reg:name "asc";
  reg:xref <https://tools.ietf.org/html/rfc6295>.
