PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#audio>
  reg:record <eac3#Resource>.

<eac3> a rdfs:Class;
  dct:identifier "audio/eac3";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/audio/eac3>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc4598/>;
  <http://www.w3.org/2002/07/owl#sameAs> <eac3#Resource>.

<eac3#Resource>
  reg:file [
      reg:file "audio/eac3";
      reg:type "template"];
  reg:name "eac3";
  reg:xref <https://tools.ietf.org/html/rfc4598>.
