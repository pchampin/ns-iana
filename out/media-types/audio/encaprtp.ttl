PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#audio>
  reg:record <encaprtp#Resource>.

<encaprtp> a rdfs:Class;
  dct:identifier "audio/encaprtp";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/audio/encaprtp>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc6849/>;
  <http://www.w3.org/2002/07/owl#sameAs> <encaprtp#Resource>.

<encaprtp#Resource>
  reg:file [
      reg:file "audio/encaprtp";
      reg:type "template"];
  reg:name "encaprtp";
  reg:xref <https://tools.ietf.org/html/rfc6849>.
