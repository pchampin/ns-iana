PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#audio>
  reg:record <rtx#Resource>.

<rtx> a rdfs:Class;
  dct:identifier "audio/rtx";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/audio/rtx>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc4588/>;
  <http://www.w3.org/2002/07/owl#sameAs> <rtx#Resource>.

<rtx#Resource>
  reg:file [
      reg:file "audio/rtx";
      reg:type "template"];
  reg:name "rtx";
  reg:xref <https://tools.ietf.org/html/rfc4588>.
