PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#image>
  reg:record <jpm#Resource>.

<jpm> a rdfs:Class;
  dct:identifier "image/jpm";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/image/jpm>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc3745/>;
  <http://www.w3.org/2002/07/owl#sameAs> <jpm#Resource>.

<jpm#Resource>
  reg:file [
      reg:file "image/jpm";
      reg:type "template"];
  reg:name "jpm";
  reg:xref <https://tools.ietf.org/html/rfc3745>.
