PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#image>
  reg:record <gif#Resource>.

<gif> a rdfs:Class;
  dct:identifier "image/gif";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/image/gif>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc2045/>,
    <https://datatracker.ietf.org/doc/rfc2046/>;
  <http://www.w3.org/2002/07/owl#sameAs> <gif#Resource>.

<gif#Resource>
  reg:file [
      reg:file "image/gif";
      reg:type "template"];
  reg:name "gif";
  reg:xref <https://tools.ietf.org/html/rfc2045>,
    <https://tools.ietf.org/html/rfc2046>.
