PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#image>
  reg:record <t38#Resource>.

<t38> a rdfs:Class;
  dct:identifier "image/t38";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/image/t38>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc3362/>;
  <http://www.w3.org/2002/07/owl#sameAs> <t38#Resource>.

<t38#Resource>
  reg:file [
      reg:file "image/t38";
      reg:type "template"];
  reg:name "t38";
  reg:xref <https://tools.ietf.org/html/rfc3362>.
