PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX reg: <https://www.w3.org/ns/assignments/reg#>
PREFIX dct: <http://purl.org/dc/terms/>

<../media-types.ttl#image>
  reg:record <bmp#Resource>.

<bmp> a rdfs:Class;
  dct:created "2016-05-16"^^xsd:date;
  dct:identifier "image/bmp";
  rdfs:definedBy <https://www.iana.org/assignments/media-types/image/bmp>;
  rdfs:seeAlso <https://datatracker.ietf.org/doc/rfc7903/>;
  <http://www.w3.org/2002/07/owl#sameAs> <bmp#Resource>.

<bmp#Resource>
  dct:date "2016-05-16"^^xsd:date;
  reg:file [
      reg:file "image/bmp";
      reg:type "template"];
  reg:name "bmp";
  reg:xref <https://tools.ietf.org/html/rfc7903>.
